<% provide(:title, "Reporte de usuario ##{@post_report.id}") %>
<h1> <%= link_to "Reporte de usuario ##{@post_report.id}", post_path(@post_report.post_id) %></h1>
<p>Usuario que reporta: <%= link_to @post_report.user.name, @post_report.user %></p>
<p>Asunto: <%= link_to @post_report.topic, "#"%><!-- TODO--></p>
<p>Mensage: <%= @post_report.message%></p>
<p>Post: <%= link_to @post_report.post.title, @post_report.post %> </p>
<p><%= link_to "eliminar reporte", @post_report, method: :delete, data: { confirm: "¿Está seguro?" } %></p>
