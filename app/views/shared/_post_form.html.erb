<%= form_for(@post) do |f|%>
    <%= render 'shared/error_messages', object: f.object %>
    <div class="field">
        <%= f.text_field :title, placeholder: "Título..."%>
        <%= f.text_area :content, placeholder:
        "Escriba su publicación..."%>
    <div>
    <%= f.submit "Post", class:"btn btn=primary" %>
<%end%>